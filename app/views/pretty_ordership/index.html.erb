<h1>All Pretty-Ordership</h1>
<table border='2'>
	<tr>
		<td>pretty_id</td>
		<td>order_id</td>
		<td>edit / delete</td>
	</tr>
	<% @pos.each do |t| %>
		<tr>
			<td><%= t.pretty_id %></td>
			<td><%= t.order_id %></td>
			<td><%= link_to 'edit' , :action => 'modify', :id => t.id %> / <%= link_to 'delete' , :action => 'delete', :id => t.id %></td>
		</tr>
	<% end %>
</table>
<p><%= link_to 'Create new pretty-ordership', 'pretty_ordership/newpo' %></p>