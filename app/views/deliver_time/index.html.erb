<h1>Deliver Time</h1>
<table border='2'>
	<tr>
		<td>deliver time</td>
		<td>deliver expense</td>
		<td>edit / delete</td>
	</tr>
	<% @deliverTimes.each do |t| %>
		<tr>
			<td><%= t.deliverTime %></td>
			<td><%= t.deliverExpense %></td>
			<td><%= link_to 'edit' , :action => 'modify', :id => t.id %> / <%= link_to 'delete' , :action => 'delete', :id => t.id %></td>
		</tr>
	<% end %>
</table>
<p><%= link_to 'Create new deliver time', 'deliver_time/newtime' %></p>
<p><%= link_to 'Back', 'admin' %></p>