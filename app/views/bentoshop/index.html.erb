<h1>Bento Shop</h1>
<table border='2'>
	<tr>
		<td>shop_name</td>
		<td>address</td>
		<td>phone</td>
		<td>city</td>
		<td>edit / delete</td>
	</tr>
	<% @bentoshops.each do |t| %>
		<tr>
			<td><%= t.shop_name %></td>
			<td><%= t.address %></td>
			<td><%= t.phone %></td>
			<td><%= t.city %></td>
			<td><%= link_to 'edit' , :action => 'modify', :id => t.id %> / <%= link_to 'delete' , :action => 'delete', :id => t.id %></td>
		</tr>
	<% end %>
</table>
<p><%= link_to 'Create new bento shop', 'bentoshop/newshop' %></p>