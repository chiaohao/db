<h1>Homepage</h1>
<%= form_for @customer, :url => {:controller => "main_page", :action => "login"} do |f| %>

<div class="field">
	<%= f.label :account %>
	<br>
	<%= f.text_field :account %>
</div>
<div class="field">
	<%= f.label :password %>
	<br>
	<%= f.text_field :password %>
</div>
<div class="actions">
	<%= f.submit %>
</div>
<% end %>